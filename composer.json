{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "bensampo/laravel-enum": "^4.1",
        "binary-cats/sanitizer": "^9.0",
        "bukashk0zzz/yml-generator": "^1.11",
        "calebporzio/parental": "^1.0",
        "cviebrock/eloquent-sluggable": "^8.0",
        "doctrine/dbal": "^3.0",
        "elasticsearch/elasticsearch": "^7.11",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.10",
        "guzzlehttp/guzzle": "^7.0.1",
        "haydenpierce/class-finder": "^0.4.3",
        "imangazaliev/didom": "^1.18",
        "jenssegers/date": "^4.0",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^9.0",
        "laravel/horizon": "^5.7",
        "laravel/octane": "^1.2",
        "laravel/sanctum": "^2.14",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "medeq/bitrix24": "dev-master",
        "mll-lab/graphql-php-scalars": "^4.1",
        "mll-lab/laravel-graphql-playground": "^2.5",
        "nuwave/lighthouse": "^5.3",
        "nwidart/laravel-modules": "^8.2",
        "prettus/l5-repository": "^2.7",
        "propaganistas/laravel-phone": "^4.3",
        "sentry/sentry-laravel": "^2.8",
        "spatie/data-transfer-object": "^3.1",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-json-api-paginate": "^1.10",
        "spatie/laravel-query-builder": "^3.3",
        "spatie/laravel-sitemap": "^6.0",
        "spiral/roadrunner": "^2.8.2",
        "staudenmeir/eloquent-json-relations": "^1.1",
        "symfony/postmark-mailer": "^5.3",
        "vitalybaev/google-merchant-feed": "^2.1",
        "wildbit/swiftmailer-postmark": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^6.2",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.6",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ray": "^1.17"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan telescope:publish --ansi",
            "@php artisan horizon:publish --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
